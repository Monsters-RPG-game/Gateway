{
  "name": "gateway",
  "version": "0.1.3",
  "description": "Gateway",
  "productName": "Gateway",
  "author": "https://github.com/Virus288",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "start": "cross-env NODE_ENV=prod node build/src/main.js",
    "start:dev": "cross-env NODE_ENV=dev TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true nodemon build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "cross-env NODE_ENV=development eslint . --ext .ts --fix",
    "listErrors": "node_modules/.bin/tsc --skipLibCheck",
    "lintStaged": "npx lint-staged",
    "test": "echo 'If you want to run all the tests, please use makefile'",
    "test:e2e": "jest --runInBand --coverage --config __tests__/jest.config.e2e.ts",
    "test:unit": "jest --coverage --config __tests__/jest.config.unit.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ]
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.0",
    "node": "^19.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@swc/core": "^1.3.41",
    "@swc/helpers": "^0.4.14",
    "@types/amqplib": "^0.10.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsutils": "^3.21.0",
    "typescript": "^5.0.2"
  }
}
