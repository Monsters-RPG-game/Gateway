{
  "name": "gateway",
  "version": "0.1.4",
  "description": "Gateway",
  "productName": "Gateway",
  "author": "https://github.com/Virus288",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "start": "cross-env NODE_ENV=prod node build/src/main.js",
    "start:dev": "concurrently 'npm run build:watch' 'cross-env NODE_ENV=dev TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true nodemon build/src/main.js'",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "cross-env NODE_ENV=development eslint . --ext .ts --fix",
    "listErrors": "node_modules/.bin/tsc --skipLibCheck",
    "lintStaged": "npx lint-staged",
    "test": "echo 'If you want to run all the tests, please use makefile'",
    "test:e2e": "cross-env NODE_ENV=test jest --coverage --config __tests__/jest.config.e2e.ts",
    "test:db": "cross-env NODE_ENV=test jest --coverage --config __tests__/jest.config.db.ts",
    "test:unit": "cross-env NODE_ENV=test jest --coverage --config __tests__/jest.config.unit.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ]
  },
  "dependencies": {
    "amqplib": "0.10.3",
    "chalk": "4.1.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-rate-limit": "^7.1.4",
    "helmet": "^7.0.0",
    "jsonwebtoken": "9.0.2",
    "moc-socket": "^0.1.2",
    "node": "21.2.0",
    "redis": "4.6.11",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "8.15.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/amqplib": "^0.10.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^20.10.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "6.14.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "5.3.3"
  }
}
