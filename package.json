{
  "name": "gateway",
  "version": "0.1.4",
  "description": "Gateway",
  "productName": "Gateway",
  "author": "https://github.com/Virus288",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "start": "cross-env NODE_ENV=production node build/src/main.js",
    "start:dev": "concurrently 'npm run build:watch' 'cross-env NODE_ENV=dev TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true nodemon build/src/main.js'",
    "start:testDev": "cross-env NODE_ENV=testDev TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true nodemon build/src/main.js",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "cross-env NODE_ENV=development eslint . --ext .ts --fix",
    "listErrors": "node_modules/.bin/tsc --skipLibCheck",
    "lintStaged": "npx lint-staged",
    "test": "echo 'If you want to run all the tests, please use makefile'",
    "test:e2e": "cross-env NODE_ENV=test jest --config __tests__/jest.config.e2e.ts",
    "test:db": "cross-env NODE_ENV=test jest --config __tests__/jest.config.db.ts",
    "test:unit": "cross-env NODE_ENV=test jest --config __tests__/jest.config.unit.ts",
    "prepare": "husky install",
    "migrate:latest:prod": "cross-env NODE_ENV=production npx knex --knexfile build/src/migrations/index.js migrate:latest",
    "migrate:rollback:prod": "cross-env NODE_ENV=production npx knex --knexfile build/src/migrations/index.js migrate:rollback",
    "migrate:latest:dev": "cross-env NODE_ENV=dev npx knex --knexfile src/migrations/index.ts migrate:latest",
    "migrate:rollback:dev": "cross-env NODE_ENV=dev npx knex --knexfile src/migrations/index.ts migrate:rollback"
  },
  "lint-staged": {
    "*.ts": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ]
  },
  "dependencies": {
    "amqplib": "0.10.3",
    "body-parser": "1.20.2",
    "chalk": "4.1.2",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "ejs": "3.1.9",
    "express": "4.18.2",
    "express-rate-limit": "7.2.0",
    "express-session": "1.18.0",
    "helmet": "7.1.0",
    "jsonwebtoken": "9.0.2",
    "knex": "3.1.0",
    "moc-socket": "0.1.2",
    "mysql2": "3.9.2",
    "node": "21.6.2",
    "node-jose": "2.2.0",
    "oidc-provider": "^6",
    "redis": "4.6.13",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0",
    "winston": "3.11.0",
    "winston-daily-rotate-file": "5.0.0",
    "ws": "8.16.0"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/amqplib": "0.10.5",
    "@types/cookie-parser": "1.4.7",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/express-session": "1.17.10",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "20.11.22",
    "@types/node-jose": "1.1.13",
    "@types/oidc-provider": "8.4.4",
    "@types/supertest": "6.0.2",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.6",
    "@types/ws": "8.5.10",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.1",
    "concurrently": "8.2.2",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "nodemon": "3.0.3",
    "prettier": "3.2.5",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
